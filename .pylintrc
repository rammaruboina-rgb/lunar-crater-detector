[MASTER]
jobs=4
unsafe-load-any-extension=False

[MESSAGES CONTROL]
disable=missing-module-docstring,
        fixme,
        no-member,
        no-name-in-module

[DESIGN]
max-args=6
max-locals=20
max-attributes=7
max-branches=12
max-statements=50

[VARIABLES]
additional-builtins=

[FORMAT]
max-line-length=100
indent-string='    '

[LOGGING]
logging-format-style=old
